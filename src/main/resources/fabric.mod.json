{
	"schemaVersion": 1,
	"id": "backpackorganizer",
	"version": "${version}",
	"name": "backpackorganizer",
	"description": "Allows for adding items with contents into an item to have a neat organizer.",
	"authors": [
		"Sentree"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/backpackorganizer/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.sentree.backpackorganizer.Backpackorganizer"
		],
		"fabric-datagen": [
			"net.sentree.backpackorganizer.BackpackorganizerDataGenerator"
		]
	},
	"mixins": [
		"backpackorganizer.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
